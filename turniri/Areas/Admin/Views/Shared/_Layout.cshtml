    var currentUser = ((turniri.Controllers.BaseController)ViewContext.Controller).CurrentUser;
}
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8" />
    <link rel="stylesheet" type="text/css" href="/Media/css/bootstrap.css" media="all" />
    <link rel="stylesheet" type="text/css" href="/Media/css/chosen.css" media="all" />
    <link rel="stylesheet" type="text/css" href="/Media/css/admin.css" media="all" />
    <link rel="stylesheet" type="text/css" href="/Media/css/jquery-ui-1.8.14.custom.css"
        media="all" />
    <script src="/Media/scripts/jquery-1.7.2.min.js" type="text/javascript"></script>

    <script src="/Media/scripts/bootstrap.js" type="text/javascript"></script>
    <script src="/Media/scripts/jquery-ui-1.8.14.custom.min.js" type="text/javascript"></script>
    <script src="/Media/scripts/jquery.ui.datepicker-ru.js" type="text/javascript"></script>
    <script src="/Media/scripts/chosen.jquery.min.js" type="text/javascript"></script>
    <script src="/Media/scripts/ajax-chosen.js" type="text/javascript"></script>
    <script type="text/javascript" src="/Media/scripts/admin/common.js"></script>
    <script type="text/javascript" src="/Media/scripts/admin/social-post.js"></script>
