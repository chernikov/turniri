@model turniri.Models.Info.ProductFilter
@{
    ViewBag.Title = "Магазин";
    Layout = "~/Areas/Default/Views/Shared/_Layout.cshtml";
}
@section header {
    <script type="text/javascript" src="/Media/scripts/default/shop.js"></script>
}
<div class="shop">
    <div class="filter" id="ShopFilter">
        @Html.Partial("ShopFilter", Model)
    </div>
    <div id="ShopContent">
    @Html.Action("Content", "Shop", new { Model })
    </div>
 </div>
